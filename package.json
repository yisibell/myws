{
  "name": "myws",
  "version": "2.5.1",
  "description": "A tiny websocket client library.",
  "type": "module",
  "main": "dist/myws.cjs",
  "module": "dist/myws.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/myws.esm.js",
      "require": "./dist/myws.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "release": "npm run build && changelogen --release && npm publish --access=public && git push --follow-tags"
  },
  "keywords": [
    "vue",
    "websocket",
    "websocket-client",
    "mitt"
  ],
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yisibell/myws.git"
  },
  "homepage": "https://github.com/yisibell/myws",
  "dependencies": {
    "mitt": "^3.0.1"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.5",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "changelogen": "^0.5.5",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "prettier": "^3.1.1",
    "rollup": "^4.9.2",
    "rollup-plugin-dts": "^6.1.0",
    "tslib": "^2.6.2",
    "typescript": "~5.3.3",
    "vue": "^2.7.16"
  }
}