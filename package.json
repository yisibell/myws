{
  "name": "myws",
  "version": "2.2.0",
  "description": "A tiny websocket client library.",
  "type": "module",
  "main": "dist/myws.cjs.js",
  "module": "dist/myws.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/myws.esm.js",
      "require": "./dist/myws.cjs.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "release": "npm run build && changelogen --release && npm publish --access=public && git push --follow-tags"
  },
  "keywords": [
    "vue",
    "websocket",
    "websocket-client",
    "mitt"
  ],
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yisibell/myws.git"
  },
  "homepage": "https://github.com/yisibell/myws",
  "dependencies": {
    "mitt": "^3.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "changelogen": "^0.5.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "rollup": "^3.27.0",
    "rollup-plugin-dts": "^5.3.0",
    "@rollup/plugin-terser": "^0.4.3",
    "tslib": "^2.6.1",
    "typescript": "~5.1.6",
    "vue": "^2.6.14"
  }
}